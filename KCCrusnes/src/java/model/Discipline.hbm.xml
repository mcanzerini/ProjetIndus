<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="model.Discipline" table="Discipline">
        <id name="id" type="long" column="id_discipline">
            <generator class="increment"/>
        </id>
        <property column="libelle_discipline" name="nom"/>
        <bag name="entraineurs" inverse="true" table="DisciplineEntraineur">
            <key column="id_discipline" not-null="true"/>
            <many-to-many column="id_personne" class="model.Entraineur"/>
        </bag>
    </class>
</hibernate-mapping>
