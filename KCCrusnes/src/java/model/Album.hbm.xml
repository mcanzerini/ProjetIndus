<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="model.Album" table="Album">
        <id name="id" type="long" column="id_album">
            <generator class="increment"/>
        </id>
        <property name="nom" column="nom"/>
        <bag name="photos" inverse="true">
            <key column="id_album" not-null="true"/>
            <one-to-many class="model.Photo"/>
        </bag>
        <!--        <one-to-many cascade="all" class="model.Evenement" name="evenement"/>-->
        <property column="date_ajout" name="dateAjout" type="calendar_date"/>
    </class>
</hibernate-mapping>
