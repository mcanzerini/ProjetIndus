<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate
   Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class name="model.Information" table="Information">
        <id name="numero" column="id_information">
            <generator class="increment"/>
        </id>
        <property name="type" column="type_information"/>
        <property name="content" column="contenu_information"/>
    </class>
</hibernate-mapping>